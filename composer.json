{
    "name": "hyperf/hyperf-skeleton",
    "type": "project",
    "keywords": [
        "php",
        "swoole",
        "framework",
        "hyperf",
        "microservice",
        "middleware"
    ],
    "description": "A coroutine framework that focuses on hyperspeed and flexible, specifically use for build microservices and middlewares.",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-redis": "*",
        "ext-swoole": ">=4.2",
        "hyperf/cache": "^1.1",
        "hyperf/command": "^1.1",
        "hyperf/config": "^1.1",
        "hyperf/contract": "^1.1",
        "hyperf/database": "^1.1",
        "hyperf/db-connection": "^1.1",
        "hyperf/devtool": "^1.1",
        "hyperf/di": "^1.1",
        "hyperf/dispatcher": "^1.1",
        "hyperf/event": "^1.1",
        "hyperf/exception-handler": "^1.1",
        "hyperf/framework": "^1.1",
        "hyperf/guzzle": "^1.1",
        "hyperf/http-server": "^1.1",
        "hyperf/logger": "^1.1",
        "hyperf/memory": "^1.1",
        "hyperf/paginator": "^1.1",
        "hyperf/pool": "^1.1",
        "hyperf/process": "^1.1",
        "hyperf/redis": "^1.1",
        "hyperf/utils": "^1.1",
        "hyperf/service-governance": "^1.1",
        "hyperf/tracer": "^1.1",
        "hyperf/websocket-server": "^1.1",
        "hyperf/view": "^1.1",
        "duncan3dc/blade": "^4.7",
        "hyperf/task": "^1.1",
        "firebase/php-jwt": "^5.0",
        "hyperf/translation": "^1.1",
        "hyperf/validation": "^1.1",
        "hyperf/amqp": "^1.1",
        "hyperf/constants": "^1.1",
        "hyperf/model-cache": "^1.1"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "^4.2",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "doctrine/common": "^2.9",
        "phpstan/phpstan": "^0.11.2",
        "hyperf/testing": "^1.1"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helper/Functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": [],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyze": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config",
        "start": "php ./bin/hyperf start"
    }
}
